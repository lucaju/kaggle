{
	"name": "scraper-kaggle",
	"version": "1.0.0",
	"description": "Scraper for Kaggle",
	"keywords": [
		"Kaggle",
		"Scraper"
	],
	"author": {
		"name": "Luciano Frizzera",
		"email": "lucaju@gmail.com",
		"url": "http://luciano.fluxo.art.br"
	},
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/lucaju/kaggle"
	},
	"type": "module",
	"main": "index.js",
	"scripts": {
		"scrape": "env-cmd -e production node src/index.mjs",
		"scrape:internal": "env-cmd -e production node src/internal.mjs",
		"scrape:internal-cluster": "env-cmd -e production node --experimental-json-modules src/internal-cluster.mjs",
		"parser": "env-cmd -e production node src/parser.mjs",
		"dev-import": "env-cmd node src/util/import",
		"dev-import-old": "env-cmd node src/util/importOldResults",
		"find": "env-cmd -e production node src/find.mjs",
		"cm": "git-cz"
	},
	"dependencies": {
		"@sendgrid/mail": "^7.4.0",
		"chalk": "^4.1.0",
		"env-cmd": "^10.0.1",
		"fs-extra": "^9.0.1",
		"jsonfile": "^6.1.0",
		"luxon": "^1.21.3",
		"mongoose": "^5.11.8",
		"node-emoji": "^1.10.0",
		"ora": "^5.1.0",
		"papaparse": "^5.3.0",
		"puppeteer": "^5.5.0",
		"puppeteer-cluster": "^0.22.0"
	},
	"devDependencies": {
		"commitizen": "^4.2.2",
		"eslint": "^7.15.0",
		"git-cz": "^4.7.6"
	},
	"config": {
		"commitizen": {
			"path": "git-cz"
		}
	}
}
